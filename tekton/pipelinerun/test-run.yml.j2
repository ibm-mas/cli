---
apiVersion: tekton.dev/v1beta1
kind: PipelineRun
metadata:
  name: mas-test-{{ build_num }}
  labels:
    tekton.dev/pipeline: mas-test
spec:
  pipelineRef:
    name: mas-test
  serviceAccountName: pipeline
  timeout: {{ pipelinerun_timeout }}
  workspaces:
    # The generated configuration files
    - name: shared-configs
      persistentVolumeClaim:
        claimName: config-pvc
  params:
    # Test Parameters
    - name: fvt_image_registry
      value: {{ lookup('env', 'FVT_IMAGE_REGISTRY') }}
    - name: fvt_artifactory_username
      value: {{ lookup('env', 'FVT_ARTIFACTORY_USERNAME') }}
    - name: fvt_artifactory_apikey
      value: {{ lookup('env', 'FVT_ARTIFACTORY_APIKEY') }}
    - name: mas_add_catalog
      value: {{ lookup('env', 'MAS_ADD_CATALOG') }}
    - name: mas_add_channel
      value: {{ lookup('env', 'MAS_ADD_CHANNEL') }}
    - name: ddp_apikey
      value: {{ lookup('env', 'DDP_APIKEY') }}
    - name: partium_username
      value: {{ lookup('env', 'PARTIUM_USERNAME') }}
    - name: partium_password
      value: {{ lookup('env', 'PARTIUM_PASSWORD') }}
    - name: devops_cos_crn
      value: {{ lookup('env', 'DEVOPS_COS_CRN') }}
    - name: fvt_version_core
      value: {{ lookup('env', 'FVT_VERSION_CORE') }}
    - name: fvt_version_iot
      value: {{ lookup('env', 'FVT_VERSION_IOT') }}
    - name: fvt_version_safety
      value: {{ lookup('env', 'FVT_VERSION_SAFETY') }}
    - name: fvt_version_monitor
      value: {{ lookup('env', 'FVT_VERSION_MONITOR') }}
    - name: fvt_version_predict
      value: {{ lookup('env', 'FVT_VERSION_PREDICT') }}
    - name: fvt_version_hputilities
      value: {{ lookup('env', 'FVT_VERSION_HPUTILITIES') }}
    - name: fvt_version_visualinspection
      value: {{ lookup('env', 'FVT_VERSION_VISUALINSPECTION') }}
    - name: fvt_version_assist
      value: {{ lookup('env', 'FVT_VERSION_ASSIST') }}
    - name: fvt_version_optimizer
      value: {{ lookup('env', 'FVT_VERSION_OPTIMIZER') }}
    - name: fvt_version_manage
      value: {{ lookup('env', 'FVT_VERSION_MANAGE') }}
    - name: fvt_version_health
      value: {{ lookup('env', 'FVT_VERSION_HEALTH') }}
    - name: fvt_version_manage_acm
      value: {{ lookup('env', 'FVT_VERSION_MANAGE_ACM') }}
    - name: fvt_version_manage_aviation
      value: {{ lookup('env', 'FVT_VERSION_MANAGE_AVIATION') }}
    - name: fvt_version_manage_hse
      value: {{ lookup('env', 'FVT_VERSION_MANAGE_HSE') }}
    - name: fvt_version_manage_nuclear
      value: {{ lookup('env', 'FVT_VERSION_MANAGE_NUCLEAR') }}
    - name: fvt_version_manage_serviceprovider
      value: {{ lookup('env', 'FVT_VERSION_MANAGE_SERVICEPROVIDER') }}
    - name: fvt_version_manage_spatial
      value: {{ lookup('env', 'FVT_VERSION_MANAGE_SPATIAL') }}
    - name: fvt_version_manage_transportation
      value: {{ lookup('env', 'FVT_VERSION_MANAGE_TRANSPORTATION') }}
    - name: fvt_version_manage_civil
      value: {{ lookup('env', 'FVT_VERSION_MANAGE_CIVIL') }}
    - name: fvt_version_manage_ic
      value: {{ lookup('env', 'FVT_VERSION_MANAGE_IC') }}
    - name: fvt_version_mobilefoundation
      value: {{ lookup('env', 'FVT_VERSION_MOBILEFOUNDATION') }}
    - name: ivt_version_core
      value: {{ lookup('env', 'IVT_VERSION_CORE') }}

taskRef:
  kind: Task
  name: mas-fvt-mobile-pytest
timeout: "0"
workspaces:
  - name: configs
    workspace: shared-configs
when:
  - input: "$(params.mas_app_channel_manage)"
    operator: notin
    values: [""]
  - input: "$(params.fvt_digest_mobile_pytest)"
    operator: notin
    values: [""]
  - input: "$(params.launchfvt_mobile_requests)"
    operator: in
    values: ["true", "True"]

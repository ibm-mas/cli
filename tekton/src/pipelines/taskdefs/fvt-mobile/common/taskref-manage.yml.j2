taskRef:
  kind: Task
  name: mas-fvt-manage
timeout: "0"
when:
  - input: "$(params.fvt_digest_manage)"
    operator: notin
    values: [""]
  - input: "$(params.mas_app_channel_manage)"
    operator: notin
    values: [""]
  - input: "civil"
    operator: in
    values: ["$(tasks.fvt-component.results.component_names[*])"]
workspaces:
  - name: configs
    workspace: shared-configs
merge-key: "{{ ACCOUNT_ID }}/{{ REGION }}/{{ CLUSTER_ID }}"

nvidia_gpu_operator:
  nfd_namespace: {{ NFD_NAMESPACE }}
  nfd_channel: {{ NFD_CHANNEL }}
  gpu_namespace: {{ GPU_NAMESPACE }}
  gpu_channel: {{ GPU_CHANNEL }}
  gpu_driver_version: {{ GPU_DRIVER_VERSION }}
  gpu_driver_repository_path: {{ GPU_DRIVER_REPOSITORY_PATH }}
{%- if CUSTOM_LABELS is defined and CUSTOM_LABELS !='' %}
  custom_labels:
{{ CUSTOM_LABELS | indent(4) }}
{%- endif %}

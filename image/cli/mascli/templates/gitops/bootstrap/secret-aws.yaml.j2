---
apiVersion: v1
kind: Secret
metadata:
  name: openshift-gitops-vault
  namespace: openshift-gitops
stringData:
  vault.yml: |
    AVP_TYPE: awssecretsmanager
    AWS_REGION: {{ SM_AWS_REGION }}
    AWS_ACCESS_KEY_ID: {{ SM_AWS_SECRET_ACCESS_KEY }}
    AWS_SECRET_ACCESS_KEY: {{ SM_AWS_ACCESS_KEY_ID }}

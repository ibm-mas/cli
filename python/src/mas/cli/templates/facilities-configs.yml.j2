mas_ws_facilities_storage_log_size: {{ mas_ws_facilities_storage_log_size }}
mas_ws_facilities_storage_userfiles_size: {{ mas_ws_facilities_storage_userfiles_size }}
mas_ws_facilities_db_maxconnpoolsize: {{ mas_ws_facilities_db_maxconnpoolsize }}
{%- if mas_ws_facilities_dwfagents is defined and mas_ws_facilities_dwfagents != ""%}
mas_ws_facilities_dwfagents:
{%- for agent in mas_ws_facilities_dwfagents %}
  - name: {{ agent.name }}
    members:
    {%- for member in agent.members %}
      - name: {{ member.name }}
        class: {{ member.class }}
    {%- endfor %}
{%- endfor %}
{%- endif %}
{% if "default" in upgrade_test_raw_param %}
  default: {{ upgrade_test_raw_param.default | default('\"\"', true) }}
{% endif %}
{% else %}
- name: {{ upgrade_test_raw_param.name }}
  type: {{ upgrade_test_raw_param.type }}
  description: {{ upgrade_test_raw_param.description | default('\"\"', true) }}

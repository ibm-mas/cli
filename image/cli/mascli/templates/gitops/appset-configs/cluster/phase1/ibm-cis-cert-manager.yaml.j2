merge-key: "{{ ACCOUNT_ID }}/{{ CLUSTER_ID }}"

ibm_cis_cert_manager:
   dns_provider: "{{ DNS_PROVIDER }}"
   ocp_cluster_domain: "{{ OCP_CLUSTER_DOMAIN }}"
   cis_apikey: "<path:{{ SECRETS_PATH }}:{{ SECRET_KEY_IBM_APIKEY }}>"
   ocp_public_cluster_domain: "{{ OCP_PUBLIC_CLUSTER_DOMAIN }}"
{% if INGRESS == 'true' %}
   ingress: true
{% else %}
   ingress: false
{% endif %}

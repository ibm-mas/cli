
# Mobile FVT - Inventory Receiving IOS
- name: fvt-inventoryreceiving-mvt-ios
  {{ lookup('template', 'taskdefs/fvt-mobile/common/taskref-testng.yml.j2') | indent(2) }}
  params:
    {{ lookup('template', 'taskdefs/fvt-mobile/common/params-testng.yml.j2') | indent(4) }}
    - name: fvt_mobile_app
      value: ir-automation-testcases
    - name: mobile_device_type
      value: ios
    - name: fvt_test_suite
      value: inventory-receiving-mvt-ios
  runAfter:
    - fvt-mobfoundation-mvt-ios
    - fvt-inventorycounting-mvt-ios
    - fvt-inspections-mvt-ios
    - fvt-asset-manager-mvt-ios
    - fvt-service-request-mvt-ios


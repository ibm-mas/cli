merge-key: "{{ ACCOUNT_ID }}/{{ CLUSTER_ID }}/{{ AISERVICE_INSTANCE_ID }}/{{ TENANT_ID }}"

ibm_aibroker_tenant:
  # aibroker
  tenant_id: "{{ TENANT_ID }}"
  aiservice_namespace: "{{ AISERVICE_NAMESPACE }}"
  aiservice_provision_tenant: "{{ AISERVICE_PROVISION_TENANT }}"
  aiservice_instance_id: "{{ AISERVICE_INSTANCE_ID }}"
  tenantNamespace: "{{ TENANT_NAMESPACE }}"
  cluster_domain: "{{ CLUSTER_DOMAIN }}"
  in_saas_env: "{{ IN_SAAS_ENV }}"
  ibm_entitlement_key: "<path:{{ SECRETS_PATH }}:{{ SECRET_KEY_IBM_ENTITLEMENT }}>"

  mas_icr_cp: "{{ MAS_ICR_CP }}"
  mas_icr_cpopen: "{{ MAS_ICR_CPOPEN }}"

  # dro
  artifactory_token: "<path:{{ SECRETS_PATH }}:{{ SECRET_KEY_ARTIFACTORY_TOKEN }}>"
  drocfg_url: "{{ DROCFG_URL }}"
  drocfg_ca: "<path:{{ SECRETS_PATH }}:{{ SECRET_KEY_DROCFG_CA }}>"
  drocfg_registration_key: "<path:{{ SECRETS_PATH }}:{{ SECRET_KEY_DROCFG_REGISTRATION_KEY }}>"

  # sls
  slscfg_ca_tenant: "<path:{{ SECRETS_PATH }}:{{ SECRET_KEY_SLSCFG_CA_TENANT }}>"
  slscfg_url: "{{ SLSCFG_URL }}"
  slscfg_registration_key: "<path:{{ SECRETS_PATH }}:{{ SECRET_KEY_SLSCFG_REGISTRATION_KEY }}>"
  aiservice_sls_subscription_id: "{{ AISERVICE_SLS_SUBSCRIPTION_ID }}"
  
  rsl_url: "{{ RSL_URL }}"
  rsl_org_id: "<path:{{ SECRETS_PATH }}:{{ SECRET_KEY_RSL_ORG_ID }}>"
  rsl_token: "<path:{{ SECRETS_PATH }}:{{ SECRET_KEY_RSL_TOKEN }}>"
  

  
  # watsonx
  aiservice_watsonxai_url: "{{ AISERVICE_WATSONXAI_URL }}"
  mas_aibroker_watsonxai_project_id: "<path:{{ SECRETS_PATH }}:{{ MAS_AIBROKER_WATSONXAI_PROJECT_ID }}>"
  mas_aibroker_watsonxai_apikey: "<path:{{ SECRETS_PATH }}:{{ SECRET_KEY_WATSONXAI_APIKEY }}>"

  # storage
  mas_aibroker_storage_host: "<path:{{ SECRETS_PATH }}:{{ SECRET_KEY_STORAGE_HOST }}>"
  mas_aibroker_storage_port: "<path:{{ SECRETS_PATH }}:{{ SECRET_KEY_STORAGE_PORT }}>"
  aiservice_storage_region: "{{ AISERVICE_STORAGE_REGION }}"
  mas_aibroker_storage_accesskey: "<path:{{ SECRETS_PATH }}:{{ SECRET_KEY_STORAGE_ACCESSKEY }}>"
  mas_aibroker_storage_secretkey: "<path:{{ SECRETS_PATH }}:{{ SECRET_KEY_STORAGE_SECRETKEY }}>"
  aiservice_storage_ssl: "{{ AISERVICE_STORAGE_SSL }}"


  #tenant 
  # S3
  aiservice_storage_provider: "{{ AISERVICE_STORAGE_PROVIDER }}"
  aiservice_s3_endpoint_url: "{{ AISERVICE_S3_ENDPOINT_URL }}"
  mas_aibroker_s3_region: "{{ MAS_AIBROKER_S3_REGION }}"
  aiservice_s3_bucket_prefix: "{{ AISERVICE_S3_BUCKET_PREFIX }}"

  tenant_entitlement_type: "{{ TENANT_ENTITLEMENT_TYPE }}"
  tenant_entitlement_start_date: "{{ TENANT_ENTITLEMENT_START_DATE }}"
  tenant_entitlement_end_date: "{{ TENANT_ENTITLEMENT_END_DATE }}"
  
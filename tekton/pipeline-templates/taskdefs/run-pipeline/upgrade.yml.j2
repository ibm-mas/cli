{% for common_param in common_params_definition.params %}
{% if common_param.name not in param_manual_setting_list %}
{% if common_param.name in params_source_target_list %}
- name: {{ common_param.name }}
  value: $(params.target_{{ common_param.name }})
{% else %}
- name: {{ common_param.name }}
  value: $(params.{{ common_param.name }})
{% endif %}
{% endif %}
{% endfor %}

{% for upgrade_param in upgrade_params_definition.params %}
{% if upgrade_param.name not in param_manual_setting_list %}
{% if upgrade_param.name in params_source_target_list %}
- name: {{ upgrade_param.name }}
  value: $(params.target_{{ upgrade_param.name }})
{% else %}
- name: {{ upgrade_param.name }}
  value: $(params.{{ upgrade_param.name }})
{% endif %}
{% endif %}
{% endfor %}
